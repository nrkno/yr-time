  "main": "src/index.js",
  "repository": "https://github.com/YR/time.git",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prepublish": "buddy build",
    "test": "cross-env NODE_ENV=test mocha test/test.js --reporter spec",
    "test-windows": "SET NODE_ENV=test&&mocha test/test.js --reporter spec"
  },
  "browser": "index.js",
  "buddy": {
    "build": [
      {
        "input": "src/",
        "output": ".",
        "bundle": false,
        "version": "es5"
      },
      {
        "input": "src/index-locales.js",
        "output": "test/lib.js"
      },
      {
        "input": "test/test-runner.html",
        "output": "docs/index.html"
      }
    ]
